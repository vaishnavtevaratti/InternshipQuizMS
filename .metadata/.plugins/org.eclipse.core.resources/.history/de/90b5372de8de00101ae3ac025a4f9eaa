package com.app.model;

public class Questions {
	public int id;
	public int quizId;
	public int text;
	public int a;
	public int b;
	public int c;
	public int d;
	public int correct;
	
	public int getId() {
		return id;
	}
	public Questions(int id, int quizId, int text, int a, int b, int c, int d, int correct) {
		super();
		this.id = id;
		this.quizId = quizId;
		this.text = text;
		this.a = a;
		this.b = b;
		this.c = c;
		this.d = d;
		this.correct = correct;
	}
	public Questions(int i, String q, String a2, String b2, String c2, String d2, char ans) {
		// TODO Auto-generated constructor stub
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getQuizId() {
		return quizId;
	}
	public void setQuizId(int quizId) {
		this.quizId = quizId;
	}
	public int getText() {
		return text;
	}
	public void setText(int text) {
		this.text = text;
	}
	public int getA() {
		return a;
	}
	public void setA(int a) {
		this.a = a;
	}
	public int getB() {
		return b;
	}
	public void setB(int b) {
		this.b = b;
	}
	public int getC() {
		return c;
	}
	public void setC(int c) {
		this.c = c;
	}
	public int getD() {
		return d;
	}
	public void setD(int d) {
		this.d = d;
	}
	public int getCorrect() {
		return correct;
	}
	public void setCorrect(int correct) {
		this.correct = correct;
	}
}
