package com.app.model;

public class Question {
	public int id ;
	public int QuizId;
	public   String text;
	public   String a;
	public   String b;
	public   String c;
	public   String d;
	public char correct;
	public Question(int id, int quizId, String text, String a, String b, String c, String d, char correct) {
		super();
		this.id = id;
		QuizId = quizId;
		this.text = text;
		this.a = a;
		this.b = b;
		this.c = c;
		this.d = d;
		this.correct = correct;
	}
	
	public Question() {
		
	}

}
